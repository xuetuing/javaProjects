<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.work.generator.mapper.PlanMapper">

    <resultMap id="BaseResultMap" type="com.example.work.generator.entity.Plan">
            <id property="id" column="id" jdbcType="INTEGER"/>
            <result property="id" column="id" jdbcType="INTEGER"/>
            <result property="areaId" column="area_id" jdbcType="INTEGER"/>
            <result property="pointId" column="point_id" jdbcType="INTEGER"/>
            <result property="workEfficiency" column="work_efficiency" jdbcType="INTEGER"/>
            <result property="startTime" column="start_time" jdbcType="TIMESTAMP"/>
            <result property="endTime" column="end_time" jdbcType="TIMESTAMP"/>
            <result property="version" column="version" jdbcType="VARCHAR"/>
            <result property="taskName" column="task_name" jdbcType="VARCHAR"/>
            <result property="constructionLogic" column="construction_logic" jdbcType="VARCHAR"/>
            <result property="status" column="status" jdbcType="TINYINT"/>
    </resultMap>

    <sql id="Base_Column_List">
        area_id,point_id,
        work_efficiency,start_time,end_time,
        version,task_name,construction_logic,
        status
    </sql>
    <insert id="insert" parameterType="com.example.work.generator.entity.Plan" useGeneratedKeys="true" keyProperty="id">
            insert into plan (<include refid="Base_Column_List"/>) values (#{areaId}, #{pointId},
            #{workEfficiency}, #{startTime}, #{endTime},
            #{version}, #{taskName}, #{constructionLogic},
            #{status})
    </insert>
</mapper>
